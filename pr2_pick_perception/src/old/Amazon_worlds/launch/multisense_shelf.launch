<!-- load standalone multisense with ranger -->
<launch>
  <!-- Use sim time -->
  <param name="/use_sim_time" type="bool" value="true"/>

   <!-- start gazebo with the Atlas -->
  <node name="gazebo" pkg="drcsim_gazebo" type="run_gazebo" args="$(find Amazon_worlds)/worlds/shelf_view1.world" output="screen" />

  <!-- Robot Description -->
  <param name="robot_description" textfile="$(find Amazon_models)/multisense_standalone/multisense_standalone.urdf"/>

  <!--<node pkg="robot_state_publisher" type="state_publisher" name="state_publisher" output="screen"/>-->

  <include file="$(find drcsim_gazebo)/launch/atlas_bringup.launch"/>

  <!-- TF Buffer Server 
  <node pkg="tf2_ros" type="buffer_server" name="tf2_buffer_server" output="screen">
    <param name="buffer_size" value="1200.0"/>
  </node>-->

  <!-- start spinning lidar -->
  <node type="rostopic" pkg="rostopic" name="spindle_init_speed" args="pub /multisense_sl/set_spindle_speed std_msgs/Float64 0.2 --once" output="screen"/>

  <!-- Stereo image processing 
  <node ns="multisense_sl" name="stereo_proc" pkg="stereo_image_proc" type="stereo_image_proc" respawn="false" output="screen">
    <param name="disparity_range" type="int" value="128"/>
    <param name="approximate_sync" type="bool" value="true"/>
  </node>-->
</launch>
